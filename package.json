{
  "name": "vue-keys-translations-manager",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "cacheDirectories": [
    "node_modules",
    "backend/node_modules",
    "frontend/node_modules",
    "frontend/dist"
  ],
  "scripts": {
    "predev": "yarn install",
    "dev:frontend": "yarn --cwd frontend dev",
    "dev:backend": "yarn --cwd backend dev",
    "dev": "run-p dev:*",
    "build": "yarn --cwd frontend build",
    "lint": "eslint frontend/src/**/* --fix",
    "test:unit": "yarn --cwd frontend test:unit",
    "test:e2e": "yarn --cwd frontend test:e2e",
    "start": "yarn --cwd backend start"
  },
  "engines": {
    "node": ">=8.11.3",
    "yarn": ">=1.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "3.1.x",
    "@vue/eslint-config-airbnb": "^3.0.4",
    "cross-env": "5.2.x",
    "imagemin-lint-staged": "0.3.x",
    "lint-staged": "8.0.x",
    "markdownlint-cli": "0.13.x",
    "npm-run-all": "4.1.x",
    "yorkie": "2.0.x"
  },
  "keywords": [
    "i18n",
    "internationalization",
    "lang",
    "language",
    "l10n",
    "locale",
    "localization",
    "multilingual",
    "translation",
    "MEVN",
    "Vue"
  ],
  "author": "chejen",
  "description": "A web application for locale's keys and translations management.",
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chejen/keys-translations-manager"
  },
  "engines": {
    "node": ">=6"
  },
  "license": "MIT"
}
